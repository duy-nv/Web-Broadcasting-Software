{
  "name": "wbs",
  "version": "1.1.2",
  "description": "Broadcasting Software for the Web",
  "repository": {
    "type": "git",
    "url": "git://github.com/RationalCoding/Web-Broadcasting-Software.git"
  },
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "standard --fix src/* && tape test/*.js",
    "build": "browserify src/index.js -t browserify-css -s WBS -o dist/wbs.js",
    "watch": "watchify src/index.js -t browserify-css -s WBS -o dist/wbs.js",
    "dev": "cross-env NODE_ENV=development webpack serve",
    "preview": "npm run watch && cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js"
  },
  "author": "Thomas Mullen",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "autoprefixer-loader": "^3.2.0",
    "babel-loader": "^8.2.2",
    "browserify-css": "^0.10.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^5.3.2",
    "less-loader": "^10.0.1",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "sass": "^1.38.1",
    "sass-loader": "^12.1.0",
    "standard": "^8.6.0",
    "style-loader": "^3.2.1",
    "tape": "^4.6.3",
    "watchify": "^4.0.0",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0"
  },
  "dependencies": {
    "cuid": "^2.1.1",
    "debounce": "^1.0.0",
    "enumerate-devices": "^1.1.1",
    "getusermedia": "^2.0.1",
    "hyperscript": "^2.0.2",
    "inherits": "^2.0.3",
    "interactjs": "^1.2.8",
    "vex-js": "^3.1.1",
    "video-stream-merger": "^3.2.0",
    "volume-meter": "^2.0.1"
  }
}
